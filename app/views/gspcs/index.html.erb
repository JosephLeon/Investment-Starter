<div class="row">
  <div class="medium-12">
    <h1>Gspcs</h1>

    <%= line_chart @data, @options %>

    <table>
      <tr>
        <th>Date</th>
        <th>Open</th>
        <th>Close</th>
        <th>High</th>
        <th>Low</th>
        <th>Volume</th>
      </tr>
     
      <% @latestgspcrecords.each do |gspc| %>
        <tr>
          <td><%= gspc.date %></td>
          <td><%= gspc.open %></td>
          <td><%= gspc.close %></td>
          <td><%= gspc.high %></td>
          <td><%= gspc.low %></td>
          <td><%= gspc.volume %></td>
        </tr>
      <% end %>
    </table>

    <table>
      <tr>
        <th>Date</th>
        <th>Open</th>
        <th>Close</th>
        <th>High</th>
        <th>Low</th>
        <th>Volume</th>
      </tr>
     
      <% @gspcs.each do |gspc| %>
        <tr>
          <td><%= gspc.date %></td>
          <td><%= gspc.open %></td>
          <td><%= gspc.close %></td>
          <td><%= gspc.high %></td>
          <td><%= gspc.low %></td>
          <td><%= gspc.volume %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>

